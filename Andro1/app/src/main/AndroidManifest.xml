<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.my.project">

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
	
		<activity
			android:label="@string/add_pet_activity_title"
			android:name=".activity.AddPetActivity"
			android:theme="@style/AddPetActivityTheme">
		
			<intent-filter>
			
				<category android:name="android.intent.category.LAUNCHER"/>
			
				<action android:name="android.intent.action.MAIN"/>
			</intent-filter>
		</activity>
	
		<activity
			android:label="@string/input3_activity_title"
			android:name=".activity.Input3Activity"
			android:theme="@style/Input3ActivityTheme"/>
	
		<activity
			android:label="@string/location_activity_title"
			android:name=".activity.LocationActivity"
			android:theme="@style/LocationActivityTheme"/>
	
		<activity
			android:label="@string/disease_activity_title"
			android:name=".activity.DiseaseActivity"
			android:theme="@style/DiseaseActivityTheme"/>
	
		<activity
			android:label="@string/walk_activity_title"
			android:name=".activity.WalkActivity"
			android:theme="@style/WalkActivityTheme"/>
	
		<activity
			android:label="@string/menu_activity_title"
			android:name=".activity.MenuActivity"
			android:theme="@style/MenuActivityTheme"/>
	
		<activity
			android:label="@string/two_activity_title"
			android:name=".activity.TwoActivity"
			android:theme="@style/TwoActivityTheme"/>
	</application>
</manifest>
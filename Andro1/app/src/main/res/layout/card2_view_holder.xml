            android:layout_width="@dimen/card2_view_holder_tonal_palette_two_constraint_layout_width"
            android:layout_height="@dimen/card2_view_holder_tonal_palette_two_constraint_layout_height"
            android:background="@color/card2_view_holder_tonal_palette_two_constraint_layout_background_color"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"

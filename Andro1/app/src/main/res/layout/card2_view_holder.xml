        app:layout_constraintTop_toBottomOf="@+id/title_and_description_constraint_layout"
        tools:layout_editor_absoluteX="233dp"
        tools:layout_editor_absoluteY="76dp">
    
        <android.support.constraint.ConstraintLayout
            android:id="@+id/surface_constraint_layout"
            android:layout_width="@dimen/card2_view_holder_surface_constraint_layout_width"
            android:layout_height="@dimen/card2_view_holder_surface_constraint_layout_height"
            android:background="@drawable/card2_view_holder_surface_constraint_layout_background"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp"/>
    
        <android.support.constraint.ConstraintLayout
            android:id="@+id/tonal_palette_constraint_layout"
            android:layout_width="@dimen/card2_view_holder_tonal_palette_constraint_layout_width"
            android:layout_height="@dimen/card2_view_holder_tonal_palette_constraint_layout_height"

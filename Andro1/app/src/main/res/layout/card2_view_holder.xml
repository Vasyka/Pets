            android:gravity="center"
            android:letterSpacing="0.02"
            android:lineSpacingMultiplier="1.14"
            android:text="Accurate feeding"
            android:textColor="@color/card2_view_holder_caption_three_text_view_text_color"
            android:textSize="@dimen/card2_view_holder_caption_three_text_view_text_size"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"

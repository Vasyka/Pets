                android:layout_marginRight="@dimen/card2_view_holder_body2_text_view_margin_end"
                android:fontFamily="@font/font_roboto_regular"
                android:gravity="left"
                android:letterSpacing="0.01"
                android:lineSpacingMultiplier="1.05"
                android:text="Diabetes in dogs is a complex disea.."
                android:textColor="@color/card2_view_holder_body2_text_view_text_color"
                android:textSize="@dimen/card2_view_holder_body2_text_view_text_size"
                app:layout_constraintBottom_toBottomOf="parent"

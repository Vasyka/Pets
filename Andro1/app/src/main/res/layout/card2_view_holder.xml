            android:layout_width="@dimen/card2_view_holder_caption_two_text_view_width"
            android:layout_height="@dimen/card2_view_holder_caption_two_text_view_height"
            android:layout_marginLeft="@dimen/card2_view_holder_caption_two_text_view_margin_start"
            android:fontFamily="@font/font_roboto_regular"
            android:gravity="center"
            android:letterSpacing="0.02"
            android:lineSpacingMultiplier="1.14"
            android:text="Accurate feeding"
            android:textColor="@color/card2_view_holder_caption_two_text_view_text_color"
            android:textSize="@dimen/card2_view_holder_caption_two_text_view_text_size"

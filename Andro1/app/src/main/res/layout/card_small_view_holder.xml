<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/card_small1"
    android:layout_width="@dimen/card_small_view_holder_card_small1_height"
    android:layout_height="@dimen/card_small_view_holder_card_small1_height"
    android:background="@drawable/card_small_view_holder_card_small1_background">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/title_and_description_constraint_layout"
        android:layout_width="0dp"
        android:layout_height="@dimen/card_small_view_holder_title_and_description_constraint_layout_height"
        android:layout_marginBottom="@dimen/card_small_view_holder_title_and_description_constraint_layout_margin_bottom"
        android:layout_marginLeft="@dimen/card_small_view_holder_title_and_description_constraint_layout_margin_start"
        android:layout_marginRight="@dimen/card_small_view_holder_title_and_description_constraint_layout_margin_end"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="146dp">
    
        <android.support.constraint.ConstraintLayout
            android:id="@+id/chips_choice_filled_enabled_constraint_layout"
            android:layout_width="@dimen/card_small_view_holder_chips_choice_filled_enabled_constraint_layout_width"
            android:layout_height="@dimen/card_small_view_holder_chips_choice_filled_enabled_constraint_layout_height"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_editor_absoluteX="68dp"
            tools:layout_editor_absoluteY="38dp">
        
            <android.support.constraint.ConstraintLayout
                android:id="@+id/surface_constraint_layout"
                android:layout_width="0dp"
                android:layout_height="@dimen/card_small_view_holder_surface_constraint_layout_height"
                android:layout_marginLeft="@dimen/card_small_view_holder_surface_constraint_layout_margin_start"
                android:layout_marginRight="@dimen/card_small_view_holder_surface_constraint_layout_margin_end"
                android:layout_marginTop="@dimen/card_small_view_holder_surface_constraint_layout_margin_top"
                android:background="@drawable/card_small_view_holder_surface_constraint_layout_background"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="parent"
                tools:layout_editor_absoluteX="1dp"
                tools:layout_editor_absoluteY="15dp"/>
        
            <android.support.constraint.ConstraintLayout
                android:id="@+id/tonal_palette_constraint_layout"
                android:layout_width="0dp"
                android:layout_height="@dimen/card_small_view_holder_tonal_palette_constraint_layout_height"
                android:layout_marginLeft="@dimen/card_small_view_holder_tonal_palette_constraint_layout_margin_start"
                android:layout_marginTop="@dimen/card_small_view_holder_tonal_palette_constraint_layout_margin_top"
                android:background="@color/card_small_view_holder_tonal_palette_constraint_layout_background_color"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="parent"
                tools:layout_editor_absoluteX="1dp"
                tools:layout_editor_absoluteY="20dp"/>
        
            <android.support.constraint.ConstraintLayout
                android:id="@+id/rectangle3_constraint_layout"
                android:layout_width="0dp"
                android:layout_height="@dimen/card_small_view_holder_rectangle3_constraint_layout_height"
                android:layout_marginLeft="@dimen/card_small_view_holder_rectangle3_constraint_layout_margin_start"
                android:layout_marginRight="@dimen/card_small_view_holder_rectangle3_constraint_layout_margin_end"
                android:layout_marginTop="@dimen/card_small_view_holder_rectangle3_constraint_layout_margin_top"
                android:background="@color/card_small_view_holder_rectangle3_constraint_layout_background_color"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="parent"
                tools:layout_editor_absoluteX="1dp"
                tools:layout_editor_absoluteY="20dp"/>
        
            <TextView
                android:id="@+id/caption_text_view"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/card_small_view_holder_caption_text_view_margin_start"
                android:layout_marginRight="@dimen/card_small_view_holder_caption_text_view_margin_end"
                android:fontFamily="@font/font_roboto_regular"
                android:gravity="center"
                android:letterSpacing="0.02"
                android:lineSpacingMultiplier="1.14"
                android:text="Treatment"
                android:textColor="@color/card_small_view_holder_caption_text_view_text_color"
                android:textSize="@dimen/card_small_view_holder_caption_text_view_text_size"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="20dp"/>
        </android.support.constraint.ConstraintLayout>
    
        <android.support.constraint.ConstraintLayout
            android:id="@+id/chips_choice_filled_enabled_two_constraint_layout"
            android:layout_width="@dimen/card_small_view_holder_chips_choice_filled_enabled_two_constraint_layout_width"
            android:layout_height="@dimen/card_small_view_holder_chips_choice_filled_enabled_two_constraint_layout_height"
            android:layout_marginBottom="@dimen/card_small_view_holder_chips_choice_filled_enabled_two_constraint_layout_margin_bottom"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="58dp">
        
            <android.support.constraint.ConstraintLayout
                android:id="@+id/surface_two_constraint_layout"
                android:layout_width="0dp"
                android:layout_height="@dimen/card_small_view_holder_surface_two_constraint_layout_height"
                android:layout_marginTop="@dimen/card_small_view_holder_surface_two_constraint_layout_margin_top"
                android:background="@drawable/card_small_view_holder_surface_two_constraint_layout_background"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="-5dp"/>
        
            <android.support.constraint.ConstraintLayout
                android:id="@+id/tonal_palette_two_constraint_layout"
                android:layout_width="0dp"
                android:layout_height="@dimen/card_small_view_holder_tonal_palette_two_constraint_layout_height"
                android:layout_marginRight="@dimen/card_small_view_holder_tonal_palette_two_constraint_layout_margin_end"
                android:background="@color/card_small_view_holder_tonal_palette_two_constraint_layout_background_color"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="0dp"/>
        
            <android.support.constraint.ConstraintLayout
                android:id="@+id/rectangle3_two_constraint_layout"
                android:layout_width="0dp"
                android:layout_height="@dimen/card_small_view_holder_rectangle3_two_constraint_layout_height"
                android:background="@color/card_small_view_holder_rectangle3_two_constraint_layout_background_color"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="0dp"/>
        
            <TextView
                android:id="@+id/caption_two_text_view"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/card_small_view_holder_caption_two_text_view_margin_start"
                android:layout_marginRight="@dimen/card_small_view_holder_caption_two_text_view_margin_end"
                android:fontFamily="@font/font_roboto_regular"
                android:gravity="center"
                android:letterSpacing="0.02"
                android:lineSpacingMultiplier="1.14"
                android:text="Treatment"
                android:textColor="@color/card_small_view_holder_caption_two_text_view_text_color"
                android:textSize="@dimen/card_small_view_holder_caption_two_text_view_text_size"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="9dp"
                tools:layout_editor_absoluteY="0dp"/>
        </android.support.constraint.ConstraintLayout>
    
        <android.support.constraint.ConstraintLayout
            android:id="@+id/headline6_color_style_constraint_layout"
            android:layout_width="0dp"
            android:layout_height="@dimen/card_small_view_holder_headline6_color_style_constraint_layout_height"
            android:layout_marginLeft="@dimen/card_small_view_holder_headline6_color_style_constraint_layout_margin_start"
            android:layout_marginRight="@dimen/card_small_view_holder_headline6_color_style_constraint_layout_margin_end"
            android:layout_marginTop="@dimen/card_small_view_holder_headline6_color_style_constraint_layout_margin_top"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="7dp"
            tools:layout_editor_absoluteY="20dp">
        
            <TextView
                android:id="@+id/headline6_text_view"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/card_small_view_holder_headline6_text_view_margin_end"
                android:layout_marginTop="@dimen/card_small_view_holder_headline6_text_view_margin_top"
                android:fontFamily="@font/font_roboto_medium"
                android:gravity="left"
                android:letterSpacing="0.01"
                android:lineSpacingMultiplier="1"
                android:text="Headline 6"
                android:textColor="@color/card_small_view_holder_headline6_text_view_text_color"
                android:textSize="@dimen/card_small_view_holder_headline6_text_view_text_size"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="4dp"/>
        </android.support.constraint.ConstraintLayout>
    </android.support.constraint.ConstraintLayout>

    <ImageView
        android:id="@+id/surface_image_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:src="@drawable/surface_2"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp"/>
</android.support.constraint.ConstraintLayout>
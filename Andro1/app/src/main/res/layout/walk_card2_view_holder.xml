<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/walk_card2"
    android:layout_width="@dimen/walk_card2_view_holder_walk_card2_height"
    android:layout_height="@dimen/walk_card2_view_holder_walk_card2_height"
    android:background="@drawable/walk_card2_view_holder_walk_card2_background">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/title_and_description_constraint_layout"
        android:layout_width="0dp"
        android:layout_height="@dimen/walk_card2_view_holder_title_and_description_constraint_layout_height"
        android:layout_marginBottom="@dimen/walk_card2_view_holder_title_and_description_constraint_layout_margin_bottom"
        android:layout_marginLeft="@dimen/walk_card2_view_holder_title_and_description_constraint_layout_margin_start"
        android:layout_marginRight="@dimen/walk_card2_view_holder_title_and_description_constraint_layout_margin_end"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="171dp">
    
        <android.support.constraint.ConstraintLayout
            android:id="@+id/body2_color_style_constraint_layout"
            android:layout_width="0dp"
            android:layout_height="@dimen/walk_card2_view_holder_body2_color_style_constraint_layout_height"
            android:layout_marginBottom="@dimen/walk_card2_view_holder_body2_color_style_constraint_layout_margin_bottom"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="26dp">
        
            <TextView
                android:id="@+id/body2_text_view"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/walk_card2_view_holder_body2_text_view_margin_end"
                android:fontFamily="@font/font_roboto_regular"
                android:gravity="left"
                android:letterSpacing="0.01"
                android:lineSpacingMultiplier="1.05"
                android:text="03:15 - 04:07"
                android:textColor="@color/walk_card2_view_holder_body2_text_view_text_color"
                android:textSize="@dimen/walk_card2_view_holder_body2_text_view_text_size"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="-1dp"/>
        </android.support.constraint.ConstraintLayout>
    
        <android.support.constraint.ConstraintLayout
            android:id="@+id/headline6_color_style_constraint_layout"
            android:layout_width="0dp"
            android:layout_height="@dimen/walk_card2_view_holder_headline6_color_style_constraint_layout_height"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp">
        
            <TextView
                android:id="@+id/headline6_text_view"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/walk_card2_view_holder_headline6_text_view_margin_end"
                android:fontFamily="@font/font_roboto_medium"
                android:gravity="left"
                android:letterSpacing="0.01"
                android:lineSpacingMultiplier="1"
                android:text="07.05.18"
                android:textColor="@color/walk_card2_view_holder_headline6_text_view_text_color"
                android:textSize="@dimen/walk_card2_view_holder_headline6_text_view_text_size"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="0dp"/>
        </android.support.constraint.ConstraintLayout>
    </android.support.constraint.ConstraintLayout>

    <com.google.android.gms.maps.MapView
        android:id="@+id/trace_map_view"
        android:layout_width="0dp"
        android:layout_height="@dimen/walk_card2_view_holder_trace_map_view_height"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp"/>
</android.support.constraint.ConstraintLayout>